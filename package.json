{
  "name": "fastify-compress",
  "version": "0.10.0",
  "description": "Fastify compression utils",
  "main": "index.js",
  "dependencies": {
    "fastify-plugin": "^1.0.0",
    "into-stream": "4.0.0",
    "is-deflate": "^1.0.0",
